    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            // "program": "${workspaceFolder}\\js-essential\\Object.js"
            "program": "${workspaceFolder}\\typescript\\src\\index.ts",
            "preLaunchTask": "tsc: build - ${workspaceFolder}\\typescript\\tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/typescript/dist/**/*.js",
            ]
        }
    ]
}
